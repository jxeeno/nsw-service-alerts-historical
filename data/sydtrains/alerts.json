          "translation": [
            {
              "text": "North Shore Line, Northern Line - Major delays",
              "language": "en"
            }
          ]
        },
        "description_text": {
          "translation": [
            {
              "text": "Allow up to 30 minutes additional journey time when travelling on the North Shore or Northern lines due to urgent track equipment repairs at Town Hall. We are working hard to rectify the issue as quickly as possible, however delays to services will continue for some time. Some train stopping patterns and departure platforms may change at short notice. Please listen to announcements, check indicator boards, and plan your trip using Transport apps for further information and updates.",
              "language": "en"
            }
          ]
        }
      }
