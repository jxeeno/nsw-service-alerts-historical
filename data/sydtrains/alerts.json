        "header_text": {
          "translation": [
            {
              "text": "Central Coast & Newcastle Line - Delays",
              "language": "en"
            }
          ]
        },
        "description_text": {
          "translation": [
            {
              "text": "Some services are delayed in both directions due to a freight train requiring mechanical repairs at Wyee earlier.\n\nPlease allow plenty of additional travel time, listen to announcements, check information screens and transport apps as some stopping patterns and departure platforms may change at short notice",
              "language": "en"
            }
          ]
        }
      }
