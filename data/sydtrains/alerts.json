        "header_text": {
          "translation": [
            {
              "text": "Bankstown Line, Inner West & Leppington Line - Major delays",
              "language": "en"
            }
          ]
        },
        "description_text": {
          "translation": [
            {
              "text": "Major Delays are occurring to services in both directions following a track maintenance vehicle with a mechanical issue at Circular Quay earlier.\n\nNormal services have resumed operating however, some gaps in service may be experienced as a result. You may have to change trains to complete your journey. \n\nBuses may supplement some trains between Glenfield and Leppington.\n\nFrequent services will be operating, some to a changed stopping pattern.\n\nPlease continue to allow plenty of additional travel time, listen to announcements and check information screens for individual service updates.",
              "language": "en"
            }
          ]
        }
      }
