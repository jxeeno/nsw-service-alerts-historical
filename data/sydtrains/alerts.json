          "translation": [
            {
              "text": "Eastern Suburbs & Illawarra Line - Partial Closure",
              "language": "en"
            }
          ]
        },
        "description_text": {
          "translation": [
            {
              "text": "Trains have been suspended between Sutherland and Cronulla in both directions due to urgent signal equipment repairs at Caringbah. Bus companies are being contacted to provide a replacement bus service with limited buses expected to be in operation around 05:00am. Please allow plenty of additional travel time, listen to announcements and refer to real time apps for train running information.",
              "language": "en"
            }
          ]
        }
      }
