        },
        "header_text": {
          "translation": [
            {
              "text": "Airport & South Line, Bankstown Line, Blue Mountains Line, Central Coast & Newcastle Line, Eastern Suburbs & Illawarra Line, Inner West & Leppington Line, North Shore Line, Northern Line, South Coast Line, Southern Highlands Line, Western Line ",
              "language": "en"
            }
          ]
        },
        "description_text": {
          "translation": [
            {
              "text": "With the current weather conditions affecting roads and public transport across Greater Sydney, you are urged to minimise non-essential travel especially around affected areas as further road closures and evacuation orders are expected.\n\nIf possible you should work from home and avoid unnecessary travel.\nFor further details go to  transportnsw.info/news/2021/flood-update-avoid-unnecessary-travel\n\nFor more information on how to prepare for storms and floods visit ses.nsw.gov.au",
              "language": "en"
            }
          ]
        }
