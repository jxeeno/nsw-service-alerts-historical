          "translation": [
            {
              "text": "Eastern Suburbs & Illawarra Line - Partial Closure",
              "language": "en"
            }
          ]
        },
        "description_text": {
          "translation": [
            {
              "text": "Trains have been suspended between Sutherland and Cronulla due to a person injured by a train at Caringbah. Buses will replace trains and are expected to start arriving at approx. 15:30hrs. No forecast for the line to reopen at this stage. Train stopping patterns may change at short notice. We advise customers to listen for announcements and check indicators before boarding trains. We do not have a forecast of when the line can be reopened.",
              "language": "en"
            }
          ]
        }
      }
