        "header_text": {
          "translation": [
            {
              "text": "Eastern Suburbs & Illawarra Line, South Coast Line - Partial Closure",
              "language": "en"
            }
          ]
        },
        "description_text": {
          "translation": [
            {
              "text": "Trains are suspended between Wollongong and Dapto due to earlier lightning strikes affecting infrastructure near Unanderra. Trains towards and from Port Kembla continue to operate as timetabled. A limited bus service is operating as an alternate service between Wollongong and Dapto. Trains are operating between Dapto and Bomaderry. Services are also being disrupted due to a medical emergency at Waterfall earlier and stopping patterns may change at short notice. Please allow plenty of additional travel time, listen to announcements and check station information screens for service updates.",
              "language": "en"
            }
          ]
        }
      }
