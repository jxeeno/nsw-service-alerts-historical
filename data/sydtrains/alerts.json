        "header_text": {
          "translation": [
            {
              "text": "Airport & South Line, Cumberland Line, Eastern Suburbs & Illawarra Line, Inner West & Leppington Line, North Shore Line, Northern Line, Olympic Park, Western Line ",
              "language": "en"
            }
          ]
        },
        "description_text": {
          "translation": [
            {
              "text": "Major disruption 20 October: Trains will run to a reduced frequency due to industrial action. \n\nSignificant delays are expected on Sydney Trains and NSW TrainLink Intercity services throughout the day. Trains will run to a reduced timetable and some services will be cancelled.\nTrains will run every 30 minutes on some Sydney Trains suburban lines, and services will be stopping at all stations so customers should expect longer journey times.\nPlease consider using alternative transport modes including regular buses, ferries or light rail services. COVIDSafe service capacities are in effect and many services will be busier than usual.",
              "language": "en"
            }
          ]
        }
