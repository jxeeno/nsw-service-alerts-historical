        "header_text": {
          "translation": [
            {
              "text": "Hunter Line - Partial Closure",
              "language": "en"
            }
          ]
        },
        "description_text": {
          "translation": [
            {
              "text": "Trains have been suspended between Maitland and Dungog/Scone due to severe weather conditions causing flooding. A replacement bus service has been arranged. \nHunter line services are experiencing delays, and if possible please avoid any non-essential travel as further flooding on the Hunter Line has been predicted.\nPlease listen to announcements, check information screens and transport apps before boarding your service.",
              "language": "en"
            }
          ]
        }
      }
