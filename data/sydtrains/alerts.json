        "header_text": {
          "translation": [
            {
              "text": "Bankstown Line - Partial Closure",
              "language": "en"
            }
          ]
        },
        "description_text": {
          "translation": [
            {
              "text": "Trains have been suspended between Lidcombe and Regents Park due to overhead wiring repairs at Regents Park. Bus companies are being contacted to operate a replacement service between Bankstown and Lidcombe until train services can resume, however we do not yet have a forecast on when buses will be on site. Major delays expected.  A limited train service will operate away from Bankstown for customers travelling to Yagoona and Birrong. This service disruption is expected to continue into this evening.",
              "language": "en"
            }
          ]
        }
      }
