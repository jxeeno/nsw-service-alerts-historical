        "header_text": {
          "translation": [
            {
              "text": "Station Update - Wynyard",
              "language": "en"
            }
          ]
        },
        "description_text": {
          "translation": [
            {
              "text": "Monday 30 November to Thursday 03 December - nightly from 8pm until 4:30am.\n\nThe George St entrance/exit to Wynyard Station, Transit Hall and Hunter Tunnel will be closed during station upgrade works. Access to/from the station is still available via Carrington St, York St & Barangaroo. \n\nPlease follow the signs and staff directions. If you need assistance, see station staff or call 9224 2959.",
              "language": "en"
            }
          ]
        }
