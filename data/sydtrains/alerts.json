          "translation": [
            {
              "text": "Inner West & Leppington Line - Good service",
              "language": "en"
            }
          ]
        },
        "description_text": {
          "translation": [
            {
              "text": "Services to/from Leppington will now terminate at/commence from Homebush instead of Strathfield due to overhead wiring repairs between Homebush and Flemington. Western Line services to/from Penrith/Emu Plains will make an additional stop at Flemington in both directions. Please continue to allow additional travel time as you may have to change trains to complete your journey and may experience longer journey times.  Repair crews are onsite. The forecast for services to return to normal has been revised forward to 00:00 Midnight tonight.",
              "language": "en"
            }
          ]
        }
      }
