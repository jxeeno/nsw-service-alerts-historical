        "header_text": {
          "translation": [
            {
              "text": "Station Update - Central",
              "language": "en"
            }
          ]
        },
        "description_text": {
          "translation": [
            {
              "text": "From 11 January to end-December 2021, the escalators and staircase connecting the corner of Pitt Street and Eddy Avenue with the Grand Concourse and L1 Light Rail stop will be closed due to station upgrade works. \n\nTo access the Grand Concourse and Platforms 1 to 12 from the street level, please use the Western Forecourt entry via Pitt Street. To access Platforms 16 to 25 from the street level, please use the Eddy Avenue entry. \n\nTo access Pitt Street or Eddy Avenue from the Grand Concourse or L1 light rail stop, please exit the station via the Western Forecourt or Northern Concourse.",
              "language": "en"
            }
          ]
        }
      }
