        "header_text": {
          "translation": [
            {
              "text": "Cumberland Line, Inner West & Leppington Line - Partial Closure",
              "language": "en"
            }
          ]
        },
        "description_text": {
          "translation": [
            {
              "text": "Trains have been suspended between Granville & Cabramatta via Merrylands in both directions due to urgent overhead wiring repairs at Guildford. Bus companies are being contacted to provide a replacement service to convey customers between Granville & Cabramatta. No forecast currently available to when buses will commence operating. Train services to the City are being diverted via Regents Park, from Cabramatta first stop Lidcombe & then as tabled. Train services from the City are being diverted via Regents Park, from Lidcombe first stop Cabramatta & then as tabled. Customers may need to change at Lidcombe, Granville or Cabramatta to complete thier journey. Please allow additional travel time, listen to announcements, check indicator screens & real time apps for service updates.",
              "language": "en"
            }
          ]
        }
      }
