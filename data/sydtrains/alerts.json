        "header_text": {
          "translation": [
            {
              "text": "Eastern Suburbs & Illawarra Line, South Coast Line - Major delays",
              "language": "en"
            }
          ]
        },
        "description_text": {
          "translation": [
            {
              "text": "due to urgent track equipment repairs at Mortdale & a train mechanical issue at Bondi Junction earlier. Stopping patterns and departure platforms may change at short notice. Buses are supplementing train services between Sutherland and Cronulla in both directions. Please listen to announcements and check indicators for next train information as we do everything we can to restore a frequency of service.",
              "language": "en"
            }
          ]
        }
      }
