        "header_text": {
          "translation": [
            {
              "text": "Station Update - Central",
              "language": "en"
            }
          ]
        },
        "description_text": {
          "translation": [
            {
              "text": "From 18 May 2020 there will be changes to Platforms 16 and 17 at Central Station due to Sydney Metro construction works -   Hoarding for the Central Walk excavation work will move from the middle of the platform to the southern end of the platform. Temporary concrete ramps will be built to connect the existing platform floor surface and the new concrete floor surface areas.  For your safety, please move carefully along the platform and follow directions from staff.  If you are travelling to Central Station and require assistance to alight from Platforms 16 and 17, please board via the front door of the fourth train carriage.  For more information about the Sydney Metro construction works, visit sydneymetro.info.",
              "language": "en"
            }
          ]
        }
      }
