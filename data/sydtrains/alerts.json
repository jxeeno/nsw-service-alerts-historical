        "header_text": {
          "translation": [
            {
              "text": "Airport & South Line, Bankstown Line, Inner West & Leppington Line - Delays",
              "language": "en"
            }
          ]
        },
        "description_text": {
          "translation": [
            {
              "text": "Delays due to urgent track repairs at Central. Bankstown Line trains will be diverted from Central first stop Museum to provide regular services. Airport & South line trains will start /end at Central. Repair crews are on site and working to restore train services as soon as possible. We will provide further information as it becomes available.\n \nFrequent trains are still operating, some with changed stops. Please allow additional travel time, check the latest travel updates via our real time apps, social channels, and station announcements.",
              "language": "en"
            }
          ]
        }
      }
