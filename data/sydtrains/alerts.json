        "header_text": {
          "translation": [
            {
              "text": "Olympic Park - Reduced services",
              "language": "en"
            }
          ]
        },
        "description_text": {
          "translation": [
            {
              "text": "Train services are running at a reduced frequency today due to significant train maintenance required following damage caused by the recent severe storms. Trains are running every 20 minutes. Customers can also utilise local bus routes 525 and 526. Please allow additional travel time, listen to announcements and check indicator boards and Transport Apps for updates.",
              "language": "en"
            }
          ]
        }
      }
