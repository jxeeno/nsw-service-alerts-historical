          "translation": [
            {
              "text": "Station Update - Central",
              "language": "en"
            }
          ]
        },
        "description_text": {
          "translation": [
            {
              "text": "From 10pm Saturday 4 July to 6am Sunday 5 July 2020 the lift between the Grand Concourse and Northern Concourse at Central Station will be closed for Sydney Metro construction works. The Suburban Tunnel will also be closed during this period. Alternative lift is available outside the Opal gates. Please follow the signs and staff directions. If you need assistance, see station staff.  For more information about the Sydney Metro construction works visit sydneymetro.info.",
              "language": "en"
            }
          ]
        }
