          "translation": [
            {
              "text": "Bankstown Line, Inner West & Leppington Line - Major delays",
              "language": "en"
            }
          ]
        },
        "description_text": {
          "translation": [
            {
              "text": "Major Delays are occurring to services in both directions due to a mechanical issue with a track maintenance vehicle at Circular Quay. \n\nTrains are operating in one direction only on the City Circle via Museum. You may have to change trains to complete your journey. \n\nService gaps are currently being experienced as a result.\n\nPlease allow plenty of additional travel time, listen to announcements and check information screens for individual service updates.",
              "language": "en"
            }
          ]
        }
