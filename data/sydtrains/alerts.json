        "header_text": {
          "translation": [
            {
              "text": "Central Coast & Newcastle Line - Partial Closure",
              "language": "en"
            }
          ]
        },
        "description_text": {
          "translation": [
            {
              "text": "Trains have been suspended between Morriset and Wyong in both directions due to a freight train with mechanical problems at Wyee. \n\nBus companies are being contacted to operate a replacement service between Morriset and Wyong, however there is no forecast for arrival at this stage.\n\nServices from the city will terminate at Wyong. Services from Newcastle will terminate at Morriset.\n\nPlease allow plenty of additional travel time, listen to announcements, check information screens and transport apps for the latest updates.",
              "language": "en"
            }
          ]
        }
      }
    },
